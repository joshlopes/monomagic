{
  "name": "Sync Google Keep Shopping List to Home Assistant",
  "version": "1.1.0",
  "slug": "google_keep_shopping_list_to_ha_sync",
  "description": "Syncs Google Keep Shopping List to Home Assistant",
  "url": "https://github.com/joshlopes/monomagic",
  "startup": "application",
  "boot": "auto",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "init": false,
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "options": {
    "GOOGLE_EMAIL": "your@email.com",
    "GOOGLE_APP_PASSWORD": "your_master_token_read_readme_for_more_info",
    "SYNC_INTERVAL": "300",
    "HA_URL": "http://homeassistant.local:8123",
    "HA_TOKEN": "your_long_lived_token"
  },
  "schema": {
    "GOOGLE_EMAIL": "str",
    "GOOGLE_APP_PASSWORD": "password",
    "SYNC_INTERVAL": "int?",
    "HA_URL": "str",
    "HA_TOKEN": "password"
  }
}
