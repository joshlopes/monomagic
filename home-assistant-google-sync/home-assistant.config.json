{
  "name": "Google Keep to HA Shopping List Sync",
  "version": "1.0.0",
  "slug": "google_keep_ha_sync",
  "description": "Syncs Google Keep Shopping List to Home Assistant",
  "startup": "application",
  "boot": "auto",
  "arch": ["aarch64", "amd64"],
  "init": false,
  "ingress": false,
  "map": ["config:rw"],
  "options": {
    "GOOGLE_EMAIL": "your@email.com",
    "GOOGLE_APP_PASSWORD": "your_app_password",
    "HA_URL": "http://homeassistant.local:8123",
    "HA_TOKEN": "your_long_lived_token",
    "SYNC_INTERVAL": "300"
  },
  "schema": {
    "GOOGLE_EMAIL": "str",
    "GOOGLE_APP_PASSWORD": "str",
    "HA_URL": "str",
    "HA_TOKEN": "str",
    "SYNC_INTERVAL": "str?"
  }
}
